<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  
  <Description about="urn:mozilla:install-manifest">
    <em:id>mozmetrics@mozilla.org</em:id>
    <em:bootstrap>true</em:bootstrap>
    <em:version>0.1</em:version>
    <em:type>2</em:type>
    
    <!-- Target Application this extension can install into, 
         with minimum and maximum supported versions. --> 
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>3.6.*</em:minVersion>
        <em:maxVersion>4.0.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    
    <!-- Front End MetaData -->
    <em:name>MozMetrics</em:name>
    <em:description>Adds usage data reporting to Sync</em:description>
    <em:creator>Matt DeBoard</em:creator>
    <em:homepageURL>https://github.com/mattdeboard/mozmetrics/</em:homepageURL>
  </Description>      
</RDF>